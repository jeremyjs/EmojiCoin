{
  "env": {
    "react-native/react-native": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react-native/all"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 6
  },
  "plugins": [
    "react",
    "react-native"
  ],
  "rules": {
    "react/display-name": "off",
    "react/no-unescaped-entities": "warn",
    "react/prop-types": "off"
  }
}
